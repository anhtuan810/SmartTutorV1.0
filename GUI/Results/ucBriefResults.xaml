<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:local="clr-namespace:GUI"
	mc:Ignorable="d"
	x:Class="GUI.ucBriefResults"
	x:Name="UserControl" Height="900" Width="1600">

	<Grid x:Name="LayoutRoot" Height="900">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="640"/>
			<ColumnDefinition Width="100*"/>
		</Grid.ColumnDefinitions>
		<Grid x:Name="Overall_Playback" Margin="0">
			<Grid.RowDefinitions>
				<RowDefinition Height="480"/>
				<RowDefinition Height="100*"/>
			</Grid.RowDefinitions>
			<local:ucMediaPlayer Margin="0" Height="480"/>
		</Grid>
		<Grid x:Name="Features" Grid.Column="1" Margin="0">
			<local:usOneFeature HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
			<local:usOneFeature HorizontalAlignment="Left" Margin="10,253,0,0" VerticalAlignment="Top"/>
			<local:usOneFeature HorizontalAlignment="Left" Margin="10,497,0,0" VerticalAlignment="Top"/>
		</Grid>
	</Grid>
</UserControl>